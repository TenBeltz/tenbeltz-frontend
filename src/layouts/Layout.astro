---
import SEO from "../components/SEO.astro"

interface Props {
	title: string
	description: string
	canonical?: string
}

const { title, description, canonical } = Astro.props
---

<!doctype html>
<html lang="en">
	<head>
		<SEO
			title={title}
			canonical={canonical}
			description={description}
		/>
	</head>
	<body>
		<slot />
	</body>
</html>
<style is:global>
</style>
